MainControl.prototype.rprintd = function(x,y,dir,s) {
    var dx = 0;
    var dy = 0;
    if      (dir == "u" ){        dy=-1; }
    else if (dir == "ur"){ dx= 1; dy=-1; }
    else if (dir == "r" ){ dx= 1;        }
    else if (dir == "dr"){ dx= 1; dy= 1; }
    else if (dir == "d" ){        dy= 1; }
    else if (dir == "dl"){ dx=-1; dy= 1; }
    else if (dir == "l" ){ dx=-1;        }
    else if (dir == "ul"){ dx=-1; dy=-1; }

    var len = s.length;
    for(var n = 0; n < len; n++) {
        var a = s.charAt(n);
        var o = G.D[G.makekey(x + dx * n,y + dy * n)];
        if (o!=null) {
            o.innerHTML = a;
        }
    }
};

